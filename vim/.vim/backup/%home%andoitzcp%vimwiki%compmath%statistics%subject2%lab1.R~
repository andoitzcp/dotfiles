fabs <- table(discoveries)
frels <- fabs/length(discoveries)
fabsacc <- as.table(cumsum(fabs))
frelsacc <- as.table(cumsum(frels))

par(mrow = c(2, 2)
plot(fabs, type="l", ylab="Freq. absolutas")
barplot(fabs, ylab="Freq. absolutas", main="Historico de descubrimientos")
plot(frels, type="l", ylab="Freq. relativas")
barplot(frels, ylab="Freq. absolutas", main="Historico de descubrimientos")
